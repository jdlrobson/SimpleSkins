{{{SKIN_START}}}
		<nav class="personal">
			{{#menu.personal}}
			<a href="{{href}}" class="{{class}}">{{text}}</a>
			{{/menu.personal}}
		</nav>
		<nav class="site">
			{{{page.toc}}}
			<h2>{{{site.name}}}</h2>
			<ul>
				{{#menu.primary}}
				<li>
					<a href="{{href}}" class="{{class}}">{{text}}</a>
				</li>
				{{/menu.primary}}
			</ul>
			{{#page.hasLanguages}}
			<h2>{{messages.otherlanguages}}</h2>
			<ul>
				{{#page.languages}}
				<li>
					<a href="{{href}}" class="{{class}}">{{text}}</a>
				</li>
				{{/page.languages}}
			</ul>
			{{/page.hasLanguages}}
			<h2>{{messages.toolbox}}</h2>
			<ul>
				{{#menu.toolbox}}
				<li>
					<a href="{{href}}" class="{{class}}">{{text}}</a>
				</li>
				{{/menu.toolbox}}
			</ul>
			{{{site.poweredby}}}
		</nav>
		{{#page.displayTitle}}<h1>{{{text}}}</h1>{{/page.displayTitle}}
		<div class="container-dali">
			<a class="lastmodbar" href="{{history.href}}">{{history.info}}</a>
			<div class="content">{{{page.html}}}</div>
		</div>
		<div class="actions">
			{{#view}}<a href="{{href}}" class="{{class}}">{{text}}</a> {{/view}}
			{{#talk}}<a href="{{href}}" class="{{class}}">{{text}}</a> {{/talk}}
			<hr>
			{{#edit}}<a href="{{href}}" class="{{class}}">{{text}}</a>{{/edit}}
			{{#watch}}<a href="{{href}}" class="{{class}}">{{text}}</a>{{/watch}}
			{{#move}}<a href="{{href}}" class="{{class}}">{{text}}</a>{{/move}}
			{{#delete}}<a href="{{href}}" class="{{class}}">{{text}}</a>{{/delete}}
			{{#restore}}<a href="{{href}}" class="{{class}}">{{text}}</a>{{/restore}}
		</div>
		<footer>
			{{#footer.info}}
				<div>
					{{{.}}}
				</div>
			{{/footer.info}}
			{{#footer.places}}
				<div>
					{{{.}}}
				</div>
			{{/footer.places}}
		</footer>
	</body>
{{{SKIN_END}}}
