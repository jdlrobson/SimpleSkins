{{{SKIN_START}}}
		<nav class="personal">
			{{#personalUrls}}
			<a href="{{href}}" class="{{class}}">{{text}}</a>
			{{/personalUrls}}
		</nav>
		<nav class="site">
			{{{page.toc}}}
			<h2>{{{site.name}}}</h2>
			<ul>
				{{#sidebarPrimaryLinks}}
				<li>
					<a href="{{href}}" class="{{class}}">{{text}}</a>
				</li>
				{{/sidebarPrimaryLinks}}
			</ul>
			{{#page.hasLanguages}}
			<h2>{{messages.otherlanguages}}</h2>
			<ul>
				{{#languageUrls}}
				<li>
					<a href="{{href}}" class="{{class}}">{{text}}</a>
				</li>
				{{/languageUrls}}
			</ul>
			{{/page.hasLanguages}}
			<h2>{{messages.toolbox}}</h2>
			<ul>
				{{#toolboxUrls}}
				<li>
					<a href="{{href}}" class="{{class}}">{{text}}</a>
				</li>
				{{/toolboxUrls}}
			</ul>
			{{{site.poweredby}}}
		</nav>
		{{#page.displayTitle}}<h1>{{{text}}}</h1>{{/page.displayTitle}}
		<div class="container-dali">
			<a class="lastmodbar" href="{{history.href}}">{{history.info}}</a>
			<div class="content">{{{bodytext}}}</div>
		</div>
		<div class="actions">
			{{#namespaces}}<a href="{{href}}" class="{{class}}">{{text}}</a> {{/namespaces}}
			<hr>
			{{#edit}}<a href="{{href}}" class="{{class}}">{{text}}</a>{{/edit}}
			{{#watch}}<a href="{{href}}" class="{{class}}">{{text}}</a>{{/watch}}
			{{#move}}<a href="{{href}}" class="{{class}}">{{text}}</a>{{/move}}
			{{#delete}}<a href="{{href}}" class="{{class}}">{{text}}</a>{{/delete}}
			{{#restore}}<a href="{{href}}" class="{{class}}">{{text}}</a>{{/restore}}
		</div>
		<footer>
			{{#footer.info}}
				<div>
					{{{.}}}
				</div>
			{{/footer.info}}
			{{#footer.places}}
				<div>
					{{{.}}}
				</div>
			{{/footer.places}}
		</footer>
	</body>
{{{SKIN_END}}}
